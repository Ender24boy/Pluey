{
    "name": "Pluey! - Tweet/Bluesky Joke Editor",
    "description": "Input message to have it be punched-out\n of tweet/Bluesky post",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": ["https://bsky.app/*", "https://x.com/*"],
        "js": ["content.js", "editText.js"]
      }
    ],
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html",
      "default_icon": "dog.png"
    },
    "permissions": [
      "activeTab"
    ]  
  }